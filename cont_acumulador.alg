algoritmo "semnome"
// Função :
// Autor :
// Data : 19/03/2014
// Seção de Declarações 
var
cont, valor, acm: inteiro

inicio
cont <- 0
acm <- 0
enquanto cont < 5 faca
         escreva("digite um numero")
         leia(valor)
         acm <- acm + valor
         cont <- cont + 1
fimenquanto

escreval("a soma total é: ", acm)

fimalgoritmo
